        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Movement

if(!dead){
    key_right = keyboard_check(vk_right);
    key_left = -keyboard_check(vk_left);
    key_jump = keyboard_check_pressed(vk_space);
    
    // React to input
    move = key_left + key_right;
    hsp = move * movespeed;
    if(vsp &lt; 10){ vsp += grav;}
    
    if(key_right != 0){
       image_xscale = 1;
    }
    else if(key_left != 0){
        image_xscale = -1;
    }
    
    if(place_meeting(x,y+1,obj_Collision)){
        vsp = key_jump * -jumpspeed;
    }
    
    // Horizontal Collision
    if(place_meeting(x+hsp,y,obj_Collision)){
        while(!place_meeting(x+sign(hsp),y,obj_Collision)){
            x += sign(hsp);
        }
        hsp = 0;
    }
    x += hsp;
    
    // Vertical Collision
    if(place_meeting(x,y+vsp,obj_Collision)){
        while(!place_meeting(x,y+sign(vsp),obj_Collision)){
            y += sign(vsp);
        }
        vsp = 0;
    }
    y += vsp;
    
    if(!atk_anim &amp;&amp; !hurt_anim &amp;&amp; !ranged_anim){
        //Check which sprite to display
        if(!place_meeting(x, y+1,obj_Collision) &amp;&amp; vsp &gt; 0){
            //Falling sprite
            //Placeholder, replace with fall sprite
            sprite_index = spr_player_fall;
            image_speed = .2;
        }
        else if(!place_meeting(x, y+1, obj_Collision)){
            //Jump
            sprite_index = spr_player_jump;
            image_speed = .4;
        }
        else if(key_right == 0 &amp;&amp; key_left == 0){
            //Standing sprite
            sprite_index = spr_Player1_Stand;
        }
        else if((key_right != 0 || key_left != 0)){
            //Running sprite
            sprite_index = spr_player_run;
            image_speed = .2;
        }
        else{
            // If all else fails, use standing sprite
            sprite_index = spr_Player1_Stand;
        }
    }
    else if(atk_anim){
        sprite_index = spr_player_melee;
    }
    else if(ranged_anim){
        sprite_index = spr_player_ranged;
    }
    else if(hurt_anim){
        sprite_index = spr_player_hurt;
    }
}
</string>
